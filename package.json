    "husky": "^8.0.1",
    "md5": "^2.3.0",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "rgb-hex": "^4.0.0",
    "vue": "^3.2.13",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/md5": "^2.3.2",
    "@types/nprogress": "^0.2.0",
    "@types/path-browserify": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
